{
  "expo": {
    "name": "docora",
    "slug": "docora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/docora_hospital.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo/docora.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/docora.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "mimeType": "Production"
        }
      ],
      "expo-secure-store"
    ],
    "permissions": [
      "CAMERA",
      "CAMERA_ROLL",
      "READ_EXTERNAL_STORAGE"
    ]
  }
}
